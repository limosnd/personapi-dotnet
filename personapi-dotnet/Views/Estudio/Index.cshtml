@model IEnumerable<personapi_dotnet.Models.Entities.Estudio>
@{
 ViewData["Title"] = "Estudios";
}
<h1>Estudios</h1>
<p><a class="btn btn-primary" asp-action="Create">Create New</a></p>
<table class="table">
 <thead><tr><th>Profesion</th><th>Persona</th><th>Fecha</th><th>Universidad</th><th></th></tr></thead>
 <tbody>
 @foreach(var item in Model){
 <tr>
 <td>@item.IdProfNavigation?.Nom</td>
 <td>@item.CcPerNavigation?.Nombre</td>
 <td>@item.Fecha</td>
 <td>@item.Univer</td>
 <td>
 <a class="btn btn-info btn-sm" asp-action="Details" asp-route-idProf="@item.IdProf" asp-route-ccPer="@item.CcPer">Details</a>
 <a class="btn btn-warning btn-sm" asp-action="Edit" asp-route-idProf="@item.IdProf" asp-route-ccPer="@item.CcPer">Edit</a>
 <a class="btn btn-danger btn-sm" asp-action="Delete" asp-route-idProf="@item.IdProf" asp-route-ccPer="@item.CcPer">Delete</a>
 </td>
 </tr>
 }
 </tbody>
</table>
